{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/saveorder",
      "methods": ["POST"],
      "dest": "/api/saveorder"
    },

    {
      "src": "/api/checkout",
      "methods": ["POST"],
      "dest": "/api/checkout"
    },

    {
      "src": "/api/signup",
      "methods": ["POST"],
      "dest": "/api/signup"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "FIREBASE_ACCOUNT_KEY": "@firebase_account_key"
    }
  }
}
